
<div class="fuck">   
  <h3>Log in</h3>
  <div class="form-group">
    <%= form_tag '/login' do %>
      Email: <%= text_field_tag :email, class: 'form-control' %>
      Password: <%= password_field_tag :password, class: 'form-control' %>
      <%= submit_tag "Submit" %>
    <% end %>
  </div>
<%= link_to "Sign up", '/signup', method: :get %>
</div>
